<Window x:Class="EasyBot.AddActionWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:EasyBot"
        mc:Ignorable="d"
        Title="Add Action" Height="100" Width="400" Background="#151515">
    
    
    
    <Grid>
        <Grid.ColumnDefinitions>
            <ColumnDefinition/>
            <ColumnDefinition/>
        </Grid.ColumnDefinitions>

        <Button Grid.Column="0" Content="MouseAction" Style="{StaticResource DarkButton}"/>
        <Button Grid.Column="1" Content="KeyBoardAction" Style="{StaticResource DarkButton}"/>

    </Grid>
</Window>
